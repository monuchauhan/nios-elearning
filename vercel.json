{
  "version": 2,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/chapter/:id", "destination": "/chapter.html" },
    { "source": "/login", "destination": "/login.html" },
    { "source": "/signup", "destination": "/signup.html" },
    { "source": "/course", "destination": "/course.html" },
    { "source": "/payment", "destination": "/payment.html" }
  ]
}
